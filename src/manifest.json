{
    "name": "Tabcycle",
    "description": "This extension allows chrome users to cycle through their tabs in a history format.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage"        
    ],
    "commands": {
        "switcheroo": {
            "suggested_key": {
                "mac": "Ctrl+W"
            },
            "description": "Switch to recent tabs"
            
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
}